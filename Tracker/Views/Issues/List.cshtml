@model Tracker.Models.Issue
@{
    ViewBag.Title = "List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<table class="table table-striped table-hover ">
    <thead>
        <tr>
            <th>Id</th>
            <th>Tytuł</th>
            <th>Opis</th>
            <th>Firma</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.Id</td>
            <td>@Model.Title</td>
            <td>@Model.Description</td>
            <td>@Model.Companyid</td>
        </tr>
    </tbody>
</table>
